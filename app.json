{
  "expo": {
    "name": "Personal_disaster_assistant_frontend",
    "slug": "Personal_disaster_assistant_frontend",
    "owner": "madara192002",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "personaldisasterassistantfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to share your safety status with your family.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app tracks your location in the background to keep your family updated on your safety.",
        "NSLocationAlwaysUsageDescription": "This app tracks your location in the background to keep your family updated on your safety."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1a592d92-0039-4185-9d41-6c78ce87bb45"
      }
    }
  }
}